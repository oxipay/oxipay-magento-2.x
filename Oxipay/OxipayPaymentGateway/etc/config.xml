<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <oxipay_gateway>                
                <active>1</active>
                <model>Oxipay\OxipayPaymentGateway\Model\Oxipay</model>                
                <order_status>complete</order_status>               
                <title>Oxipay</title>
                <description>Pay the easier way</description>
                <api_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <test_mode>0</test_mode>
                <gateway_url>https://secure.oxipay.com.au/Checkout?platform=default</gateway_url>
                <group>oxipay</group>                
            </oxipay_gateway>
        </payment>
    </default>
</config>
